-- insert into students values (1,'luffy','67',123457890);

-- select * from students;



create table stud(
Id int auto_increment primary key,     -- using a auto_increment for Id 
name varchar(30),
marks int,
mobno int unique                          -- usinf for unique mobile num
);

select * from stud;
insert into stud(name,marks,mobno)values ('luffy','67',123457890);
insert into stud(name,marks,mobno)values 
('zoro','62',123247890),
('sanji','34',123987890),
('choper','92',1232087890);

select * from stud;


select * from stud where id = 1;      -- select row in list


select name from stud;               -- particular column select in list


select name,marks from stud where Id = 3;    -- select name,marks in list


select * from stud where marks >80;          -- finding a mark in about > 80 marks in list


select * from stud where marks <80;          -- finding a marks in less than < 80 marks in list


select * from stud where marks !=  60 and  name != 'luffy';                 -- remove the selecting values in list\


-- operators in MySQl:-                <,>,<=,>=,=,!=,




